<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".NuevaCita">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <SearchView
            android:id="@+id/buscarcitaconsultorio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/purple_700"></SearchView>

        <ImageView
            android:id="@+id/Consultorio1"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:src="@drawable/consultorio1"
            android:onClick="SeleccionConsultorio"></ImageView>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:src="@drawable/consultorio2"></ImageView>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:src="@drawable/consultorio3"></ImageView>

    </LinearLayout>



</androidx.coordinatorlayout.widget.CoordinatorLayout>